<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /webbanhang/
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ app/index.php?url=$1 [L,QSA]
</IfModule>

